# Defines built in syntax highlighter themes.
#
# Must be included in program resources in UTF-8 format.
# ------------------------------------------------------------------------------

Theme _Legacy_ CodeSnip Classic
  Brush *
    Attr Space *,*,{}
    Attr Comment *,800000,{italic}
    Attr ReservedWord *,000000,{bold}
    Attr NonreservedKeyword *,000000,{italic}
    Attr Event *,000000,{italic}
    Attr Identifier *,000000,{}
    Attr Symbol *,000000,{}
    Attr String *,800080,{}
    Attr Character *,800080,{}
    Attr Number *,000080,{}
    Attr Float *,000080,{}
    Attr Hexadecimal *,000080,{}
    Attr Preprocessor *,008000,{}
    Attr Assembler *,000000,{italic}

