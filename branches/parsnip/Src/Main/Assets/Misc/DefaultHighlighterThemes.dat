    Attr Symbol *,000000,{}
    Attr String *,800000,{}
    Attr Character *,800000,{}
    Attr Number *,800000,{}
    Attr Float *,800000,{}
    Attr Hexadecimal *,800000,{}
    Attr Preprocessor *,800000,{}
    Attr Assembler *,000000,{}

